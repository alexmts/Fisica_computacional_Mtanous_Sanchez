   "source": [
    "a=10\n",
    "\n",
    "g(x)=sqrt(complex(a-(x*x)))\n",
    "f(x)=(x*tan(x))-g(x)\n",
    "df(x)=tan(x)+(x*sec(x)*sec(x))+(x/g(x))"
   ]
