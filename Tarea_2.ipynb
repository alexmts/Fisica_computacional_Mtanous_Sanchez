   ],
   "source": [
    "function raiz(b)\n",
    "    x=b\n",
    "\n",
    "    for i in 1:20\n",
    "        \n",
    "        x=x-(f(x)/df(x))\n",
    "        \n",
    "    end\n",
    "    \n",
    "    return x\n",
    "    \n",
    "end"
   ]
